{
  "base_keymap": "VSCode",
  "relative_line_numbers": true,
  "scroll_beyond_last_line": "off",
  "gutter": {
    "line_numbers": true
  },
  "minimap": {
    "show": "never"
  },
  "features": {
    "edit_prediction_provider": "none"
  },
  "collaboration_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "buffer_line_height": "comfortable",
  "ui_font_size": 16,
  "buffer_font_size": 16.0,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Macchiato"
  },
  "vim_mode": true
}
